<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android" android:title="@string/setting_title">
	<PreferenceCategory android:title="@string/setting_title">
		<Preference android:title="@string/setting_version" android:key="VersionDescription" android:selectable="false" android:summary="@string/setting_version_summary" />
	</PreferenceCategory>
	<PreferenceCategory android:title="@string/setting_test">
		<CheckBoxPreference android:title="@string/setting_producer" android:key="TestProducer" android:defaultValue="true" android:summary="@string/setting_producer_summary" />
	</PreferenceCategory>
  <PreferenceCategory android:title="@string/setting_mobilecenter">
		<EditTextPreference android:key="MobileCenterKey" />
  </PreferenceCategory>
	<PreferenceCategory android:title="@string/setting_functions">
    <EditTextPreference android:key="RemoteFunctionsUrl" android:summary="@string/setting_functions_summary" />
  </PreferenceCategory>
	<PreferenceCategory android:title="@string/setting_document">
    <EditTextPreference android:key="RemoteDocumentDbUrl" android:summary="@string/setting_document_summary" />
  </PreferenceCategory>
	<PreferenceCategory android:title="@string/setting_notificationhub">
    <EditTextPreference android:key="NotificationsName" android:title="@string/setting_notificationhub_title" />
		<EditTextPreference android:key="NotificationsConnectionString" />
  </PreferenceCategory>
	<PreferenceCategory android:title="@string/setting_embeddedsocial">
		<EditTextPreference android:key="EmbeddedSocialKey" />
  </PreferenceCategory>
	<PreferenceCategory android:title="@string/setting_diagnostics">
		<Preference android:key="UserReferenceKey" android:selectable="false" android:summary="@string/setting_diagnostics_summary" />
	</PreferenceCategory>
</PreferenceScreen>